<?xml version='1.0' encoding='utf-8'?>
<!--Here is defined where the annotations of the entities
generated for JPA going to appear. In this case is field based-->
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">
    <persistence-unit-metadata>
        <persistence-unit-defaults>
            <access>FIELD</access>
        </persistence-unit-defaults>
    </persistence-unit-metadata>
</entity-mappings>