<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.hcl.cnp.patientservice.domain.PatientStateEntity">
        <table name="patient_state" schema="openmrs" catalog=""/>
        <attributes>
            <id name="patientStateId">
                <column name="patient_state_id"/>
            </id>
            <basic name="startDate">
                <column name="start_date"/>
            </basic>
            <basic name="endDate">
                <column name="end_date"/>
            </basic>
            <basic name="dateCreated">
                <column name="date_created"/>
            </basic>
            <basic name="dateChanged">
                <column name="date_changed"/>
            </basic>
            <basic name="voided">
                <column name="voided"/>
            </basic>
            <basic name="dateVoided">
                <column name="date_voided"/>
            </basic>
            <basic name="voidReason">
                <column name="void_reason"/>
            </basic>
            <basic name="uuid">
                <column name="uuid"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
