<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.hcl.cnp.patientservice.domain.NoteEntity">
        <table name="note" schema="openmrs" catalog=""/>
        <attributes>
            <id name="noteId">
                <column name="note_id"/>
            </id>
            <basic name="noteType">
                <column name="note_type"/>
            </basic>
            <basic name="text">
                <column name="text"/>
            </basic>
            <basic name="priority">
                <column name="priority"/>
            </basic>
            <basic name="dateCreated">
                <column name="date_created"/>
            </basic>
            <basic name="dateChanged">
                <column name="date_changed"/>
            </basic>
            <basic name="uuid">
                <column name="uuid"/>
            </basic>
            <many-to-one name="obsByObsId" optional="true" target-entity="com.hcl.cnp.patientservice.domain.ObsEntity">
                <join-column name="obs_id" nullable="false" referenced-column-name="obs_id"/>
            </many-to-one>
            <many-to-one name="noteByParent" optional="true"
                         target-entity="com.hcl.cnp.patientservice.domain.NoteEntity">
                <join-column name="parent" nullable="false" referenced-column-name="note_id"/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
