<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.hcl.cnp.patientservice.domain.AllergyReactionEntity">
        <table name="allergy_reaction" schema="openmrs" catalog=""/>
        <attributes>
            <id name="allergyReactionId">
                <column name="allergy_reaction_id"/>
            </id>
            <basic name="reactionNonCoded">
                <column name="reaction_non_coded"/>
            </basic>
            <basic name="uuid">
                <column name="uuid"/>
            </basic>
            <many-to-one name="allergyByAllergyId" optional="true"
                         target-entity="com.hcl.cnp.patientservice.domain.AllergyEntity">
                <join-column name="allergy_id" referenced-column-name="allergy_id"/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
